+ ./prog -d nan
d is nan

+ ./prog -d inf
d is inf

+ ./prog -d -inf
d is -inf

+ ./prog -d infinity
d is inf

+ ./prog -d -infinity
d is -inf

+ ./prog -d 0
d is 0.00000000000000000

+ ./prog -d -0
d is -0.00000000000000000

+ ./prog -d 1
d is 1.00000000000000000

+ ./prog -d -1
d is -1.00000000000000000

+ ./prog -d 0.0
d is 0.00000000000000000

+ ./prog -d -0.0
d is -0.00000000000000000

+ ./prog -d -0.1
d is -0.10000000000000001

+ ./prog -d 0.1
d is 0.10000000000000001

+ ./prog -d 9.2e1
d is 92.00000000000000000

+ ./prog -d 99.23e2
d is 9923.00000000000000000

+ ./prog -d 999.234e3
d is 999234.00000000000000000

+ ./prog -d 1e308
d is .00000000000000000

+ ./prog -d 1e309
prog: unable to convert '-d 1e309': Numerical argument out of domain

+ ./prog -d 0x1p10
d is 1024.00000000000000000

+ ./prog -d 0x1p20
